hi LineNr guifg=#AAAAAA guibg=#111111

"highlight GitGutterAdd  guifg=#009900 guibg=#111111
"highlight GitGutterChange  guifg=#bbbb00 guibg=#111111
"highlight GitGutterDelete  guifg=#ff2222 guibg=#111111

set lines=58 columns=180
"remove the scrollbars on macvim
set guioptions=

"set macligatures
"set guifont=Monaco\ for\ Powerline:h14
"this is cool too and is install on mac by default
set guifont=SourceCodePro+Powerline+Awesome\ Regular:h15
set linespace=-1
